      <table id="example2" class="table table-bordered table-hover table-striped" style="width:100%">
                  <thead>
                  <tr>
                    <th style="width:7%">No</th>
                    <th>Nama Game</th>
                    <th>Category</th>
                    <th>Nominal</th>
                    <th style="width:15%; ">Status</th>
                    <th style="width:19%">Actions</th>
                  </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < voucher.length; i++){ %>
                      <tr>
                        <td><%= i + 1  %> </td>
                        <td><%= voucher[i].name %> </td>
                        <td><%= voucher[i].category.name%> </td>
                        <td>
                            <% voucher[i].nominals.forEach(nominal => { %>

<!-- GIMANA XARANYAAA????!! -->

                                   <% if(voucher[i].coinName === 'Silver') {%>
                                       <span class="badge badge-secondary">
                                           <%= nominal.coinName %><%= nominal.price %> 
                                       </span>
                                   <% } else if(voucher[i].coinName === 'GOLD') { %>
                                        <span class="badge badge-warning">
                                           <%= nominal.coinName %><%= nominal.price %> 
                                        </span>
                                  <% } else if(voucher[i].coinName === 'Jewel') {%>
                                        <span class="badge badge-primary">
                                           <%= nominal.coinName %><%= nominal.price %> 
                                        </span>  
                                  <% } else {%> 
                                      <span class="badge badge-warning">
                                           <%= nominal.coinName %><%= nominal.price %> 
                                      </span>
                                  <% } %>

                                <%  });  %> 
                        </td>

                        <td style="text-align: center">
                          <form action="/voucher/status/<%= voucher[i]._id %>?_method=PUT" method="POST">
                             <button class="" 
                             style="cursor: pointer;
                                    background: transparent;
                                    border:0;">
                           <% if(voucher[i].status === 'Y') { %>
                               <input type="checkbox" name="my-checkbox" checked data-bootstrap-switch data-on-color="success" data-off-color="danger" >
                            <% } else {%>
                              <input type="checkbox" name="my-checkbox" unchecked data-bootstrap-switch data-on-color="success" data-off-color="danger" >
                           <% } %> 
                          <!-- </form> -->
                          </button>
                        </td>
                     
                        <td style="text-align: center">
                          <form action="/voucher/delete/<%= voucher[i]._id %>?_method=DELETE" method="POST" style="display: inline-block;">
                            <a href="/voucher/edit/<%= voucher[i]._id %>" class="btn btn-warning">
                              <i class="fas fa-pencil-alt" ></i>
                              Edit
                            </a>
                            <button href="" class="btn btn-danger">
                              <i class="fas fa-trash-alt"></i>
                              Delete
                            </button>
                          </form>
                        </td>
                      </tr>
                   <% } %>  
                  </tbody>
                </table>
